CAPI=2:

name: AD:utility:ad_tcl_vivado_ip:1.0.0
description: Files to help generate vivado fpga projects using analog devices HDL tcl scripts

filesets:
  src:
    files:
    - adi_ip_xilinx.tcl : {file_type: tclSource}
    - adi_xilinx_device_info_enc.tcl : {file_type: tclSource}
    - interfaces_ip.tcl : {file_type: tclSource}

  dep:
    depend:
      - AD:utility:ad_tcl_generic
    
targets:
  default: &default
    filesets: [src, dep]
